<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:tag="gameBoardWalkableElement"
    tools:context=".BoardElements.Tiles.BaseSquareFragment"
    android:clipChildren="false"
    android:clipToPadding="false"
    >

    <ImageView
        android:id="@+id/gameBoardSquareBaseImage"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:src="@drawable/tile_cropped"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:scaleType="fitEnd"/>

    <ImageView
        android:id="@+id/gameBoardSquareOverlayImage"
        android:layout_width="45dp"
        android:layout_height="0dp"
        android:layout_marginBottom="18dp"
        android:scaleType="fitEnd"
        android:src="@drawable/transparency"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.533"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>